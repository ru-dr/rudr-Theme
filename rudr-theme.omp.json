{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "owm",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "{{ if .Error }}{{ if contains .Error \"API key\" }}ğŸ”‘{{ else if contains .Error \"location\" }}ğŸ“{{ else if contains .Error \"network\" }}ğŸ“¡{{ else }}â—{{ end }}{{ else }}{{if eq .WeatherIcon \"01d\"}}â˜€ï¸{{else if eq .WeatherIcon \"01n\"}}ğŸŒ™{{else if eq .WeatherIcon \"02d\"}}ğŸŒ¤{{else if eq .WeatherIcon \"02n\"}}ğŸŒ¥{{else if or (eq .WeatherIcon \"03d\") (eq .WeatherIcon \"03n\")}}â˜ï¸{{else if or (eq .WeatherIcon \"09d\") (eq .WeatherIcon \"09n\")}}ğŸŒ§{{else if eq .WeatherIcon \"10d\"}}ğŸŒ¦{{else if eq .WeatherIcon \"10n\"}}ğŸŒ§{{else if or (eq .WeatherIcon \"11d\") (eq .WeatherIcon \"11n\")}}â›ˆ{{else if or (eq .WeatherIcon \"13d\") (eq .WeatherIcon \"13n\")}}â„ï¸{{else if or (eq .WeatherIcon \"50d\") (eq .WeatherIcon \"50n\")}}ğŸŒ«{{end}} {{.Temperature}}{{.UnitIcon}}{{ end }} ",
          "properties": {
            "api_key": "4b738b5b4fea93ec488af30a1ae403a7",
            "location": "MAHESANA,GUJARAT,IN",
            "units": "metric",
            "http_timeout": 20,
            "cache_timeout": 10
          }
        },
        {
          "foreground": "#15C2CB",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#F141A8",
          "properties": {
            "branch_icon": "\ue0a0 "
          },
          "style": "plain",
          "template": "<#F3EFF5>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#5EADF2",
          "style": "plain",
          "template": "{{if .Version}}<#F3EFF5>is</> \uf487 {{.Version}} {{end}}",
          "type": "project"
        },
        {
          "foreground": "#44FFD2",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#F3EFF5>via</> \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#FE4A49",
          "style": "plain",
          "template": "<#F3EFF5>and</> \ue71e {{.Full}} ",
          "type": "npm"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#94ffa2",
          "style": "plain",
          "template": " <#ffffff>MEM:</> {{ .PhysicalPercentUsed }}% ",
          "type": "sysinfo"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#193549",
          "template": "{{ if not .Error }}{{ .Icon }} {{ .Percentage }}%{{ end }} ",
          "properties": {
            "discharging_icon": "ğŸª«",
            "charging_icon": "âš¡",
            "charged_icon": "ğŸ”‹"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFE45E",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
