{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "owm",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#FFFFFF",
          "background": "#87CEEB",
          "template": "{{ if .Error }}{{ if contains .Error \"API key\" }}üîë{{ else if contains .Error \"location\" }}üìç{{ else if contains .Error \"network\" }}üì°{{ else }}‚ùì{{ end }}{{ else }}{{.Weather}} ({{.Temperature}}{{.UnitIcon}}){{ end }}",
          "properties": {
            "api_key": "4b738b5b4fea93ec488af30a1ae403a7",
            "location": "MAHESANA,GUJARAT,IN",
            "units": "metric",
            "http_timeout": 20,
            "cache_timeout": 10
          }
        },
        {
          "foreground": "#FFB3BA",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#BAFFC9",
          "properties": {
            "branch_icon": "\ue0a0 "
          },
          "style": "plain",
          "template": "<#FFFFFF>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#BAE1FF",
          "style": "plain",
          "template": "{{if .Version}}<#FFFFFF>is</> \uf487 {{.Version}} {{end}}",
          "type": "project"
        },
        {
          "foreground": "#FFFFBA",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#FFFFFF>via</> \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#FFDFBA",
          "style": "plain",
          "template": "<#FFFFFF>and</> \ue71e {{.Full}} ",
          "type": "npm"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#BAFFC9",
          "style": "plain",
          "template": " <#FFFFFF>MEM:</> {{ .PhysicalPercentUsed }}% ",
          "type": "sysinfo"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#FFB3BA",
          "template": "{{ if not .Error }}{{ .Icon }} {{ .Percentage }}%{{ end }} ",
          "properties": {
            "discharging_icon": "ü™´",
            "charging_icon": "‚ö°",
            "charged_icon": "üîã"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFDFBA",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
