{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
                    "type": "owm",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b6",
                    "foreground": "#ffffff",
                    "background": "#389c09",
                    "invert_powerline": true,
                    "properties": {
                        "apikey": "4b738b5b4fea93ec488af30a1ae403a7",
                        "location": "MAHESANA,GUJARAT,IN",
                        "units": "metric",
                        "enable_hyperlink": true,
                        "http_timeout": 400,
                        "cache_timeout": 15,
                        "template": "[{{.Weather}} ({{.Temperature}}{{.UnitIcon}})]({{.URL}})"
                    }
                },
        {
          "foreground": "#FF6B6B",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#4ECDC4",
          "properties": {
            "branch_icon": "\ue0a0 "
          },
          "style": "plain",
          "template": "<#FFFFFF>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#FFA500",
          "style": "plain",
          "template": "{{if .Version}}<#FFFFFF>is</> \uf487 {{.Version}} {{end}}",
          "type": "project"
        },
        {
          "foreground": "#9B59B6",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#FFFFFF>via</> \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#2ECC71",
          "style": "plain",
          "template": "<#FFFFFF>and</> \ue71e {{.Full}} ",
          "type": "npm"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#F39C12",
          "style": "plain",
          "template": " <#FFFFFF>MEM:</> {{ .PhysicalPercentUsed }}% ",
          "type": "sysinfo"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#3498DB",
          "template": "{{ if not .Error }}{{ .Icon }} {{ .Percentage }}%{{ end }} ",
          "properties": {
            "discharging_icon": "ðŸª«",
            "charging_icon": "âš¡",
            "charged_icon": "ðŸ”‹"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E74C3C",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
